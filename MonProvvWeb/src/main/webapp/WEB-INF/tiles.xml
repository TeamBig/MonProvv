<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
	<definition name="template" template="/template/template.jsp">
        <put-attribute name="header" value="/template/header.jsp" />
        <put-attribute name="breadcrumb" value="/template/breadcrumb.jsp" />
        <put-attribute name="menu" value="/template/menu.jsp" />
        <put-attribute name="alert" value="/template/alert.jsp" />
        <put-attribute name="footer" value="/template/footer.jsp" />
		<put-attribute name="titolo" value="#TEMPLATE#" />        
    </definition>	
    
    <definition name="403" extends="template">
    	<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/empty.jsp" />
		<put-attribute name="titolo" value="403.title" />
	</definition>

	<definition name="404" extends="template">
   		<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/empty.jsp" />
		<put-attribute name="titolo" value="404.title" />
	</definition>
	<definition name="500" extends="template">
   		<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/empty.jsp" />
		<put-attribute name="titolo" value="500.title" />
	</definition>
	
	<definition name="timeout" extends="template">
		<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/empty.jsp" />
		<put-attribute name="titolo" value="timeout.title" />
	</definition>
	
	<definition name="logout" extends="template">
		<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/empty.jsp" />
		<put-attribute name="titolo" value="logout.title" />
	</definition>

	<definition name="exceptionPage" extends="template">
		<put-attribute name="breadcrumb" value="" />
		<put-attribute name="content" value="/views/exceptionPage.jsp" />
		<put-attribute name="titolo" value="exceptionPage.title" />
	</definition>
	
	 
    <definition name="home" extends="template">
		<put-attribute name="content" value="/views/home.jsp" />
		<put-attribute name="titolo" value="home.title" />
	</definition>
   
    

</tiles-definitions>